    css: |
      font-family: Arial;font-size: 21px;color: #FF0000;
  - name: Italic
    className: italic
    css: |
      font-family: Arial;font-size: 21px;font-style: italic
  - name: Heading 1
    className: heading1
    css: |
      font-family: Arial;font-size: 36px;
  - name: Heading 2
    className: heading2
